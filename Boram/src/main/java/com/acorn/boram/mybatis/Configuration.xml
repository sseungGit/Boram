<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  <configuration>
  	<!-- 자주 사용하는 type 의 별칭을 등록 해 놓고  Mapper xml 에서 사용할수 있다. -->
	<typeAliases>
		<typeAlias type="com.acorn.boram.review.dto.KjReviewDto" alias="kjReviewDto"/>
	</typeAliases>
	<!-- 필요한 Mapper xml 등록하기  -->
	<mappers>
		<mapper resource="com/acorn/boram/mybatis/ReviewMapper.xml"/>
	</mappers>	
  </configuration>